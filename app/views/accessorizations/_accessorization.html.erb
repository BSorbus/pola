<td>
  <%= accessorization.project.present? ? link_to(accessorization.project.number, project_path(accessorization.project)) : '' %>
</td>
<td>
  <%= accessorization.user.present? ? link_to(accessorization.user.name, user_path(accessorization.user)) : '' %>
</td>
<td>
  <%= accessorization.role.present? ? link_to(accessorization.role.name, role_path(accessorization.role)) : '' %>
</td>
<td>
  <%#= link_to ' ', 
      edit_project_accessorization_path(id: accessorization.id, project_id: accessorization.project_id), 
      :class => "glyphicon glyphicon-edit", title: 'Edit', rel: 'tooltip' %>
  <%#= link_to ' ', 
      project_accessorization_path(id: accessorization.id, project_id: accessorization.project_id),
      method: :delete,
      data: { confirm: "Are you sure?" }, 
      :class => "glyphicon glyphicon-trash", title: 'Destroy', rel: 'tooltip' %>
</td>
