<div class="row">
  <div class="col-sm-12">
    <div class="row">
      <h3><span class="glyphicon glyphicon-flag" aria-hidden="true"> 
        <%= t "pages.user.index.title" %>  
        </span>
      </h3>
    </div> <!--/row -->
  </div> <!--/col-sm-12 -->

  <div class="col-sm-12">
    <table id="users-table" class="hover cell-border" cellspacing="0" width="100%", data-source="<%= datatables_index_users_path(format: :json) %>">
      <thead>
        <tr>
          <th>id</th>
          <th class='col-sm-2'><%= User.human_attribute_name(:name) %></th>
          <th class='col-sm-1'><%= User.human_attribute_name(:last_sign_in_ip) %></th>
          <th class='col-sm-1'><%= User.human_attribute_name(:last_sign_in_at) %></th>
          <th class='col-sm-1'><%= User.human_attribute_name(:current_sign_in_ip) %></th>
          <th class='col-sm-1'><%= User.human_attribute_name(:current_sign_in_at) %></th>
          <th class='col-sm-2'><%= User.human_attribute_name(:note) %></th>
          <th class='col-sm-4'><%= User.human_attribute_name(:flat_assigned_events) %></th>
        </tr>
      </thead>
      <tbody>
        <!-- data from ajax -->
      </tbody>
    </table>
  </div> <!--/col-sm-12 -->


</div> <!--/row -->
