<p id="notice"><%= notice %></p>

<h1>Point Files</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Load date</th>
      <th>Load file</th>
      <th>Status</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @point_files.each do |point_file| %>
      <tr>
        <td><%= point_file.project %></td>
        <td><%= point_file.load_date %></td>
        <td><%= point_file.load_file %></td>
        <td><%= point_file.status %></td>
        <td><%= point_file.note %></td>
        <td><%= link_to 'Show', point_file %></td>
        <td><%= link_to 'Edit', edit_point_file_path(point_file) %></td>
        <td><%= link_to 'Destroy', point_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Point File', new_point_file_path %>
