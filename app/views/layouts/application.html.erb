<!DOCTYPE html>
<html>
  <head>
    <title>Proca</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body class="site">
    <%= render 'layouts/header' %>
    <%= yield(:upper_part_page) %>
    <main class="site-content">
      <%= render 'layouts/messages' %>
      <%= yield :form_errors %>       
      <%= yield %>
    </main>
    <%= render 'layouts/footer' %>
  </body>
</html>
