<p id="notice"><%= notice %></p>

<h1>Opinions</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>User</th>
      <th>Sec22 rate</th>
      <th>Sec22</th>
      <th>Sec23 rate</th>
      <th>Sec23</th>
      <th>Sec24 rate</th>
      <th>Sec24</th>
      <th>Sec25 rate</th>
      <th>Sec25</th>
      <th>Sec28 rate</th>
      <th>Sec28</th>
      <th>Sec33 rate</th>
      <th>Sec33</th>
      <th>Sec41</th>
      <th>Sec42</th>
      <th>Sec43</th>
      <th>Sec51 rate</th>
      <th>Sec51</th>
      <th>Sec61</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @opinions.each do |opinion| %>
      <tr>
        <td><%= opinion.project %></td>
        <td><%= opinion.user %></td>
        <td><%= opinion.sec22_rate %></td>
        <td><%= opinion.sec22 %></td>
        <td><%= opinion.sec23_rate %></td>
        <td><%= opinion.sec23 %></td>
        <td><%= opinion.sec24_rate %></td>
        <td><%= opinion.sec24 %></td>
        <td><%= opinion.sec25_rate %></td>
        <td><%= opinion.sec25 %></td>
        <td><%= opinion.sec28_rate %></td>
        <td><%= opinion.sec28 %></td>
        <td><%= opinion.sec33_rate %></td>
        <td><%= opinion.sec33 %></td>
        <td><%= opinion.sec41 %></td>
        <td><%= opinion.sec42 %></td>
        <td><%= opinion.sec43 %></td>
        <td><%= opinion.sec51_rate %></td>
        <td><%= opinion.sec51 %></td>
        <td><%= opinion.sec61 %></td>
        <td><%= link_to 'Show', opinion %></td>
        <td><%= link_to 'Edit', edit_opinion_path(opinion) %></td>
        <td><%= link_to 'Destroy', opinion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

