# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

# Ignore secretfiles.
/config/secrets.yml
/config/deploy.rb

# Ignore uploaded files.
/public/uploads/*
